{
	"name": "flarum-lang/marathi",
	"type": "flarum-extension",
	"description": "Marathi language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/marathi",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"mr-locale",
		"marathi-locale",
		"marathi"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/marathi/issues",
		"source": "https://github.com/flarum-lang/marathi"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Marathi",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "mr",
			"title": "Marathi"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
